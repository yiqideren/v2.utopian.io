{
  "name": "@utopian/client",
  "version": "2.0.0",
  "license": "GPL-3.0",
  "author": "Utopian Company SRLs",
  "description": "Utopian.io frontend.",
  "productName": "Utopian",
  "cordovaId": "io.utopian.app",
  "private": true,
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.4.0",
    "yarn": ">= 1.12.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "scripts": {
    "build": "quasar build -m ssr -t mat",
    "dev": "quasar dev -m ssr -t mat",
    "debug": "node --inspect=127.0.0.1:9229 ./node_modules/quasar-cli/bin/quasar dev -t mat",
    "update:i18n": "node ../i18n/lib/index.js",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js,.vue src",
    "serve:dev:ngrok": "ngrok http 8080 --region eu",
    "serve:pwa": "quasar serve ./dist/pwa-mat --port 3000",
    "serve:dist": "nodemon dist/ssr-mat/index.js",
    "serve:test:coverage": "quasar serve test/coverage/lcov-report/ --port 8790",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8791",
    "test": "yarn run lint",
    "test:jest": "jest --coverage",
    "test:jest:watch": "jest --watch",
    "test:webdriver": "wdio ./test/webdriver/config/wdio.conf.js",
    "audit:snyk": "snyk test --org=info-fth --json > ./test/audits/snyk.json",
    "audit:node_modules": "yarn audit --json > ./test/audits/node_modules_audit.json",
    "audit:licenses": "nlf --summary detail > ./test/audits/licenses.txt",
    "audit:lighthouse": "lighthouse http://localhost:3000 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js",
    "vue-devtools": "./node_modules/.bin/vue-devtools",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@octokit/rest": "15.18.0",
    "@sentry/browser": "4.4.0",
    "@steemit/steem-js": "0.7.4",
    "@utopian/i18n": "1.0.0",
    "@utopian/testing": "1.0.0",
    "aes-js": "3.1.2",
    "axios": "0.18.0",
    "b2a": "1.0.10",
    "babel-eslint": "10.0.1",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "concurrently": "4.1.0",
    "css-loader": "1.0.1",
    "dotenv": "6.1.0",
    "eslint": "5.9.0",
    "eslint-config-standard": "12.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "4.7.1",
    "extra-watch-webpack-plugin": "1.0.3",
    "fs": "0.0.2",
    "html-to-text": "4.0.0",
    "jest": "23.6.0",
    "jsonwebtoken": "8.4.0",
    "lint-staged": "8.1.0",
    "marked": "0.5.2",
    "patch-package": "6.0.5",
    "postinstall-postinstall": "2.0.0",
    "pug": "2.0.3",
    "pug-plain-loader": "1.0.0",
    "quasar-cli": "0.17.22",
    "register-service-worker": "1.5.2",
    "sanitize-html": "1.19.1",
    "slugify": "1.3.3",
    "strip-ansi": "3.0.1",
    "turndown": "5.0.1",
    "turndown-plugin-gfm": "1.0.2",
    "typeface-lato": "0.0.54",
    "typeface-noto-sans": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "typeface-source-sans-pro": "0.0.54",
    "vue": "2.5.17",
    "vue-croppa": "1.3.8",
    "vue-i18n": "8.3.2",
    "vue-router": "3.0.2",
    "vuelidate": "0.7.4",
    "vuex": "3.0.1",
    "webdriverio": "4.14.0"
  }
}
